      "enabled": true,
      "check_interval_minutes": 5,
      "metrics": [
        {
          "name": "experiment_activity",
          "query": "count(runs) by experiment",
          "threshold": 0,
          "alert_if": "== 0 for 1h"
        },
        {
          "name": "failed_runs_rate",
          "query": "rate(runs{status='FAILED'}[1h])",
          "threshold": 0.1,
          "alert_if": "> 0.1"
        },
        {
          "name": "artifact_upload_failures",
          "query": "rate(artifact_upload_errors[1h])",
          "threshold": 0.05,
          "alert_if": "> 0.05"
        }
      ]
    },
    
    "performance_monitoring": {
      "run_duration": {
        "alert_threshold_hours": 24,
        "warning_threshold_hours": 12
      },
      
      "artifact_size": {
        "alert_threshold_mb": 1000,
        "warning_threshold_mb": 500
      },
      
      "concurrent_runs": {
        "max_per_experiment": 10,
        "max_total": 100
      }
    },
    
    "alerting": {
      "channels": {
        "slack": "#mlflow-alerts",
        "email": "mlops-team@goat-prediction.com",
        "pagerduty": "mlflow-service"
      },
      
      "rules": [
        {
          "name": "experiment_inactivity",
          "condition": "experiment_activity == 0 for 1h",
          "severity": "warning",
          "message": "Experiment {experiment} has been inactive for 1 hour"
        },
        {
          "name": "high_failure_rate",
          "condition": "failed_runs_rate > 0.1",
          "severity": "critical",
          "message": "High failure rate in experiment {experiment}: {value}"
        },
        {
          "name": "long_running_experiment",
          "condition": "run_duration > 24h",
          "severity": "warning",
          "message": "Experiment run {run_id} has been running for more than 24 hours"
        }
      ]
    }
  },
  
  "experiment_backup": {
    "enabled": true,
    "schedule": {
      "daily": "02:00 UTC",
      "weekly": "sunday 03:00 UTC",
      "monthly": "first 04:00 UTC"
    },
    
    "retention": {
      "daily": 30,
      "weekly": 8,
      "monthly": 12
    },
    
    "content": {
      "experiment_metadata": true,
      "run_data": true,
      "metrics": true,
      "parameters": true,
      "tags": true,
      "artifacts_metadata": true,
      "full_artifacts": false
    },
    
    "storage": {
      "primary": "s3://goat-prediction-mlflow-backups/experiments",
      "secondary": "gcs://goat-prediction-backups/mlflow/experiments"
    }
  },
  
  "experiment_retention_policy": {
    "run_retention_days": 365,
    "artifact_retention_days": 730,
    "metrics_retention_days": 1095,
    
    "auto_archive": {
      "enabled": true,
      "days_inactive": 180,
      "exclude_experiments": ["football_match_winner", "football_over_under"]
    },
    
    "cleanup": {
      "failed_runs_after_days": 7,
      "incomplete_runs_after_days": 1,
      "temporary_files_after_hours": 24
    }
  },
  
  "git_integration": {
    "enabled": true,
    "repository": "https://github.com/goat-prediction/goat-prediction-ultimate",
    "branch": "main",
    
    "requirements": {
      "commit_before_run": true,
      "pull_before_run": true,
      "tag_on_success": false
    },
    
    "metadata": {
      "track_git_commit": true,
      "track_git_branch": true,
      "track_git_origin": true,
      "track_source_code": true
    },
    
    "hooks": {
      "pre_run": "scripts/mlflow/pre_run_hook.sh",
      "post_run_success": "scripts/mlflow/post_run_success_hook.sh",
      "post_run_failure": "scripts/mlflow/post_run_failure_hook.sh"
    }
  },
  
  "experiment_templates_advanced": {
    "neural_network_training": {
      "base_config": {
        "framework": "pytorch",
        "device": "cuda",
        "mixed_precision": true
      },
      
      "tracking_config": {
        "log_gradients": true,
        "log_weights": false,
        "log_activations": false,
        "log_histograms": true,
        "log_graph": true
      },
      
      "checkpointing": {
        "save_best_only": true,
        "monitor": "val_loss",
        "mode": "min",
        "save_freq": "epoch",
        "max_checkpoints": 5
      }
    },
    
    "hyperparameter_tuning": {
      "search_strategy": "bayesian",
      "search_space": "auto",
      "n_trials": 100,
      "timeout_hours": 24,
      "early_stopping": true,
      "patience": 10,
      
      "parallel_config": {
        "n_jobs": 4,
        "backend": "loky",
        "pre_dispatch": "2*n_jobs"
      }
    }
  },
  
  "experiment_comparison": {
    "default_metrics": ["accuracy", "roc_auc", "log_loss", "roi", "sharpe_ratio"],
    "default_params": ["algorithm", "n_estimators", "learning_rate", "max_depth"],
    
    "visualizations": {
      "parallel_coordinates": true,
      "scatter_plots": true,
      "contour_plots": false,
      "hyperparameter_importance": true
    },
    
    "export_formats": ["csv", "json", "html", "pdf"]
  },
  
  "webhook_integrations": {
    "slack": {
      "enabled": true,
      "webhook_url": "${SLACK_MLFLOW_WEBHOOK}",
      "events": ["run_started", "run_completed", "run_failed", "model_registered", "model_transitioned"]
    },
    
    "datadog": {
      "enabled": true,
      "api_key": "${DATADOG_API_KEY}",
      "metrics_prefix": "mlflow.experiment"
    },
    
    "custom_webhooks": [
      {
        "name": "model_registry_sync",
        "url": "https://internal-api.goat-prediction.com/mlflow/webhook",
        "events": ["run_completed"],
        "secret": "${WEBHOOK_SECRET}"
      }
    ]
  },
  
  "api_endpoints": {
    "experiment_management": {
      "create_experiment": "/api/2.0/mlflow/experiments/create",
      "get_experiment": "/api/2.0/mlflow/experiments/get",
      "search_experiments": "/api/2.0/mlflow/experiments/search",
      "update_experiment": "/api/2.0/mlflow/experiments/update",
      "delete_experiment": "/api/2.0/mlflow/experiments/delete"
    },
    
    "run_management": {
      "create_run": "/api/2.0/mlflow/runs/create",
      "get_run": "/api/2.0/mlflow/runs/get",
      "search_runs": "/api/2.0/mlflow/runs/search",
      "update_run": "/api/2.0/mlflow/runs/update",
      "delete_run": "/api/2.0/mlflow/runs/delete",
      "log_metric": "/api/2.0/mlflow/runs/log-metric",
      "log_param": "/api/2.0/mlflow/runs/log-parameter",
      "log_batch": "/api/2.0/mlflow/runs/log-batch",
      "log_artifact": "/api/2.0/mlflow/artifacts/log-artifact",
      "list_artifacts": "/api/2.0/mlflow/artifacts/list-artifacts"
    }
  }
}
