  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:frontend": "cd frontend/web-app && npm run dev",
    "dev:backend": "cd backend/api-gateway && uvicorn src.main:app --reload --host 0.0.0.0 --port 8000",
    "dev:ml": "cd backend/prediction-engine && python src/main.py --dev",
    "dev:admin": "cd frontend/admin-dashboard && npm run dev",
    "build": "npm run build:frontend && npm run build:backend && npm run build:ml",
    "build:frontend": "cd frontend/web-app && npm run build",
    "build:backend": "cd backend/api-gateway && npm run build",
    "build:ml": "cd backend/prediction-engine && python -m py_compile src/**/*.py",
    "start": "concurrently \"npm:start:*\"",
    "start:frontend": "cd frontend/web-app && npm run start",
    "start:backend": "cd backend/api-gateway && npm run start",
    "start:ml": "cd backend/prediction-engine && python src/main.py",
    "test": "concurrently \"npm:test:*\"",
    "test:frontend": "cd frontend/web-app && npm run test",
    "test:backend": "cd backend/api-gateway && python -m pytest",
    "test:ml": "cd backend/prediction-engine && python -m pytest",
    "test:e2e": "cd tests && npm run test:e2e",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:frontend": "cd frontend/web-app && npm run lint",
    "lint:backend": "cd backend/api-gateway && python -m black . --check && python -m flake8",
    "lint:ml": "cd backend/prediction-engine && python -m black . --check && python -m flake8",
    "lint:fix": "concurrently \"npm:lint:fix:*\"",
    "lint:fix:frontend": "cd frontend/web-app && npm run lint:fix",
    "lint:fix:backend": "cd backend/api-gateway && python -m black . && python -m isort .",
    "lint:fix:ml": "cd backend/prediction-engine && python -m black . && python -m isort .",
    "type-check": "concurrently \"npm:type-check:*\"",
    "type-check:frontend": "cd frontend/web-app && npm run type-check",
    "type-check:backend": "cd backend/api-gateway && python -m mypy .",
    "type-check:ml": "cd backend/prediction-engine && python -m mypy .",
    "format": "concurrently \"npm:format:*\"",
    "format:frontend": "cd frontend/web-app && npm run format",
    "format:backend": "cd backend/api-gateway && python -m black . && python -m isort .",
    "format:ml": "cd backend/prediction-engine && python -m black . && python -m isort .",
    "security": "concurrently \"npm:security:*\"",
    "security:frontend": "cd frontend/web-app && npm run security",
    "security:backend": "cd backend && npm audit --production",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker system prune -af --volumes",
    "db:migrate": "cd backend/api-gateway && alembic upgrade head",
    "db:rollback": "cd backend/api-gateway && alembic downgrade -1",
    "db:seed": "cd backend/api-gateway && python scripts/seed_data.py",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "ml:train": "cd backend/prediction-engine && python scripts/train_models.py",
    "ml:predict": "cd backend/prediction-engine && python scripts/predict.py",
    "ml:backtest": "cd backend/prediction-engine && python scripts/backtest.py",
    "data:collect": "cd backend/prediction-engine && python scripts/collect_data.py",
    "data:process": "cd backend/prediction-engine && python scripts/process_data.py",
    "monitoring:start": "docker-compose -f docker-compose.monitoring.yml up -d",
    "monitoring:stop": "docker-compose -f docker-compose.monitoring.yml down",
    "docs:build": "cd docs && mkdocs build",
    "docs:serve": "cd docs && mkdocs serve",
    "storybook": "cd frontend/web-app && npm run storybook",
    "storybook:build": "cd frontend/web-app && npm run storybook:build",
    "preview": "npm run build && npm run start",
    "clean": "rimraf frontend/web-app/.next frontend/web-app/node_modules/.cache frontend/web-app/coverage frontend/web-app/dist",
    "clean:all": "npm run clean && rimraf node_modules frontend/web-app/node_modules frontend/admin-dashboard/node_modules",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npm run setup",
    "setup": "concurrently \"npm:setup:*\"",
    "setup:frontend": "cd frontend/web-app && npm install",
    "setup:backend": "cd backend/api-gateway && pip install -r requirements.txt",
    "setup:ml": "cd backend/prediction-engine && pip install -r requirements.txt",
    "setup:admin": "cd frontend/admin-dashboard && npm install",
    "health": "node scripts/health-check.js",
    "deploy:staging": "node scripts/deploy.js --env staging",
    "deploy:production": "node scripts/deploy.js --env production",
    "ci:test": "npm run lint && npm run type-check && npm run test",
    "ci:build": "npm run build",
    "benchmark": "cd tests && npm run benchmark",
    "performance": "cd tests && npm run performance",
    "audit": "npm audit --production && cd frontend/web-app && npm audit",
    "update": "npm update && cd frontend/web-app && npm update",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "workspaces": [
    "frontend/web-app",
    "frontend/admin-dashboard",
    "frontend/landing-page",
    "backend/api-gateway",
    "backend/prediction-engine"
  ],
